age,\r\n            to: i*qtyOnPage+qtyOnPage\r\n        })\r\n    }\r\n\r\n    return (\r\n       <>\r\n            {pageUrls.map( p => <a key={p.title} id={p.title} className={numbActive===p.title?\"receivers-list-number-border\" : \"\"} onClick={ ()=>changePageFn(p.from, p.to, p.title)}>{p.title}</a>)}\r\n       </>\r\n    );\r\n}\r\n\r\nexport default Pagination;","C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\Home\\Contact\\ErrorMessage.js",["124"],"import React, { useState } from 'react';\r\n\r\nconst ErrorMessage = ({formError}) => {\r\n    return(\r\n        <>\r\n            <p className=\"contact-form-err\">{formError}</p>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default ErrorMessage;","C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\DonateStuff\\DonateStuff.js",[],"C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\DonateStuff\\DonateOrange\\DonateOrange.js",[],"C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\DonateStuff\\DonateBanner\\DonateBanner.js",["125"],"C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\DonateStuff\\DonateForm\\DonateForm.js",["126"],"C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\DonateStuff\\DonateNavigation\\DonateNavigation.js",[],"C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\DonateStuff\\DonateNavigation\\NavLoggedIn.js",[],"C:\\AAACodersLab\\Zadania\\Donate-Stuff - React App\\donate-stuff\\src\\components\\SignOut\\SignOut.js",[],{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","severity":1,"message":"132","line":2,"column":8,"nodeType":"133","messageId":"134","endLine":2,"endColumn":15},{"ruleId":"135","severity":1,"message":"136","line":16,"column":9,"nodeType":"137","messageId":"138","endLine":35,"endColumn":10},{"ruleId":"139","sever